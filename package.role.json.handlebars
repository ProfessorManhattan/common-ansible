{
  "devDependencies": {
    "@megabytelabs/prettier-config-ansible": "^1.1.4"
  },
  "keywords": [
    "{{galaxy_info.role_name}}",
    "ansible",
    "archlinux",
    "centos",
    "debian",
    "doctor",
    "fedora",
    "install",
    "installdoctor",
    "macos",
    "megabytelabs",
    "molecule",
    "professormanhattan",
    "role",
    "ubuntu",
    "windows"
  ],
  "lint-staged": {
    "logo.png": [
      "task image:resize -- 200 240",
      "task image:compress --"
    ],
    "./**/*.py": [
      "black",
      "flake8"
    ],
    "**/*vault.yml": [
      "bash .common/scripts/ansible/vault.sh"
    ]
  },
  "prettier": "@megabytelabs/prettier-config-ansible"
}
